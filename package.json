  "private": true,
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "@sap-cloud-sdk/http-client": "^3.0.1",
    "@sap/cds": "^6",
    "@sap/cds-odata-v2-adapter-proxy": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4",
    "express-graphql": "^0.12.0",
    "graphql": "^15.3.0",
    "hdb": "^0.19.0"
  },
  "devDependencies": {
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/ux-specification": "^1.102.17",
    "@sapui5/ts-types": "^1.92.2",
    "cds-swagger-ui-express": "^0.5.0",
    "eslint": "^7.32.0",
    "sqlite3": "^5.0.4"
  },
  "scripts": {
    "start": "cds run"
  },
  "cds": {
    "i18n": {
      "default_language": "en",
      "folders": [
        "_i18n",
        "i18n",
        "assets/i18n"
